## Question 1
start_index=2000
end_index=3200
list_final=[]
while start_index <=end_index:
    if start_index % 7 == 0 and start_index % 5 != 0:
        list_final.append(start_index)
    start_index=start_index+1
print (list_final)

## Question 2
f_name=input('Enter your First Name:')
l_name=input('Enter your Last Name:')
val=f_name+" "+l_name
print(val[::-1])


#Question 3
import math
diameter=12
